<<%- include('../partials/header') %>

<ul>
  <% devskills.forEach(function(t) { %>
    <li>
      <a href="/devskills/<%= t.id %>"><%= t.devskill %></a>
      
      <%= t.done ? "" : "" %>
      
      <form 
        action="/devskills/<%= t.id %>?_method=DELETE"
        class="delete-form"
        method="POST"
        
      >
      <button type="submit">X</button>

    </form>
    </li>
  <% }) %>
</ul>
<a href="/devskills/new">Add Devskills</a>
<%- include('../partials/footer') %>